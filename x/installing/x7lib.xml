&libXcomposite-md5sum;  libXcomposite-&libXcomposite-version;.tar.xz
&libXrender-md5sum;  libXrender-&libXrender-version;.tar.xz
&libXcursor-md5sum;  libXcursor-&libXcursor-version;.tar.xz
&libXdamage-md5sum;  libXdamage-&libXdamage-version;.tar.xz
&libfontenc-md5sum;  libfontenc-&libfontenc-version;.tar.xz
&libXfont2-md5sum;  libXfont2-&libXfont2-version;.tar.xz
&libXft-md5sum;  libXft-&libXft-version;.tar.xz
&libXi-md5sum;  libXi-&libXi-version;.tar.bz2
&libXinerama-md5sum;  libXinerama-&libXinerama-version;.tar.xz
&libXrandr-md5sum;  libXrandr-&libXrandr-version;.tar.xz
&libXres-md5sum;  libXres-&libXres-version;.tar.xz
&libXtst-md5sum;  libXtst-&libXtst-version;.tar.xz
&libXv-md5sum;  libXv-&libXv-version;.tar.xz
